{
    "manufacturer": "Joshua Zander",
    "keyboard_name": "keyseebee",
    "maintainer": "Sefodopo",
    "bootloader": "stm32-dfu",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "encoder": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "oled": true
    },
    "build": {
        "lto": true
    },
    "matrix_pins": {
        "direct": [
            ["B3", "A15", "B15", "B13", "A8", "B14"],
            ["B4", "B5", "B6", "B7", "B1", "B2"],
            ["B0", "B10", "B11", "A7", "A6", "A1"],
            ["NO_PIN", "A0", "A2", "A3", "A4", "A5"]
        ]
    },
    "processor": "STM32F072",
    "url": "",
    "usb": {
        "device_version": "0.4.1",
        "pid": "0x9EF7",
        "vid": "0xFEED"
    },
    "encoder": {
        "rotary": [
            {
                "pin_a": "F1",
                "pin_b": "F0",
                "resolution": 4
            }

        ]
    },
    "split": {
        "enabled": true,
        "bootmagic": {
            "matrix": [4, 5]
        },
        "handedness": {
            "pin": "B12"
        },
        "matrix_pins": {
            "right": {
                "direct": [
                    ["A8", "B15", "A15", "B14", "B13", "B10"],
                    ["A6", "A7", "B0", "B2", "B1", "B11"],
                    ["A1", "A2", "B7", "A3", "A4", "A5"],
                    ["B5", "B6", "B4", "B3", "A0", "NO_PIN"]
                ]
            }
        },
        "transport": {
            "protocol": "serial"
        }
    },
    "dynamic_keymap": {
        "layer_count": 7
    },
    "layouts": {
        "LAYOUT_split_3x6_5": {
